public class 冒泡 {
public static void main(String[] args) {
int mp[]=new int[5];
system.out.println("请输入五个数");
Scanner s=new Scanner(System.in);
	for(int i=0;i<s.length;i++)
	{
	mp[i]=s.nextInt();
		for(int j=1;j>0;j--)
		{
			if(mp[j]>mp[j-1])
			{
				int temp = mp[j];
				mp[j]=mp[j-1];
				mp[j-1]=temp;
			}
		}
	}
	}
	}
	
	
#### 测试用例
1,2,3,4,5
4，7,5,11,0
32,4,2,1,4,

